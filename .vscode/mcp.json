{
	"servers": {
		},
		"firecrawl/firecrawl-mcp-server": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"firecrawl-mcp@latest"
			],
			"env": {
				"FIRECRAWL_API_KEY": "${input:api_key}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"io.github.wonderwhy-er/desktop-commander": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@wonderwhy-er/desktop-commander@0.2.32"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.2.32"
		},
		"io.github.ChromeDevTools/chrome-devtools-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"chrome-devtools-mcp@0.15.0"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.15.0"
		}
	},
	"inputs": [
		{
			"id": "token",
			"type": "promptString",
			"description": "",
			"password": true
		},
		{
			"id": "api_key",
			"type": "promptString",
			"description": "your API key",
			"password": true
		}
	]
}